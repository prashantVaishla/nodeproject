        <div class="modal fade" id="myModal" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header" style="height:50px;">
                        <h4 style="margin-top:0px">Product quality case has been created.</h4>
                    </div>
                    <div class="modal-body" style="padding:40px 50px;">
                        <table>
                            <tr>
                                <td>
                                    <p>Case Id:</p>
                                </td>
                                <td>
                                    <input type="text" disabled id="lblMongoDBid" value="" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>Summary:</p>
                                </td>
                                <td>
                                    <input type="text" disabled id="lblComplaintSummary" value="" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>Bar Code:</p>
                                </td>
                                <td>
                                    <input type="text" disabled id="lblBarCode" value="" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>TPNB: </p>
                                </td>
                                <td>
                                    <input type="text" disabled id="lblTPNB1"/>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>TPNC</p>
                                </td>
                                <td>
                                    <input type="text" disabled="disabled" id="lblTPNC1" value=""/>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>Store number:</p>
                                </td>
                                <td>
                                    <input type="text" disabled id="lblStoreNumber1" value=""/>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary" data-dismiss="modal" onclick="window.location.href='/home'">OK</button>                      
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="errorProductLook" role="dialog" style="margin-top:75px">
            <div class="modal-dialog" style="width:300px;">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header" style="height:50px">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 style="margin-top:0px">Error</h4>
                    </div>
                    <div class="modal-body">
                        <label>You have entered an invalid value</label>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="errorStoreLookUp" role="dialog" style="margin-top:75px">
            <div class="modal-dialog" style="width:300px;">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header" style="height:50px">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 style="margin-top:0px">Error</h4>
                    </div>
                    <div class="modal-body">
                        <label>You have entered an invalid store number</label>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="errorForEmptyFields" role="dialog" style="margin-top:75px">
            <div class="modal-dialog" style="width:300px;">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header" style="height:50px">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 style="margin-top:0px">Error</h4>
                    </div>
                    <div class="modal-body">
                        <label>One or more mandatory fields are empty!</label>
                    </div>
                </div>
            </div>
        </div>
    </div>
